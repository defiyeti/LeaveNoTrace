<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.leavenotrace" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LogIn"
            android:label="LeaveNoTrace" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AccountSettings"
            android:label="@string/title_activity_account_settings" >
        </activity>
        <activity
            android:name=".AddNewData"
            android:label="@string/title_activity_add_new_data" >
        </activity>
        <activity
            android:name=".UpdatePastUsage"
            android:label="@string/title_activity_update_past_usage" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="MainActivity" >
        </activity>
        <activity
            android:name=".SignUpOne"
            android:label="@string/title_activity_sign_up_one" >
        </activity>
        <activity
            android:name=".PastUsage"
            android:label="@string/title_activity_past_usage" >
        </activity>
    </application>

</manifest>
